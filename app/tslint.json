{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {

      "no-console": false,
      "variable-name": [
          true,
          "allow-snake-case",
          "allow-leading-underscore"
      ],
      "no-string-literal": false,
      "only-arrow-functions": false,
      "no-commented-code": false,
      "array-type": false,
      "no-var-requires": false
  },

  "rulesDirectory": []
}